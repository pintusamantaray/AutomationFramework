package day10;

public class TestPc {
	
	int age;
	String name;
	String dept;
	
	void display()
	{
		System.out.println(name +" "+age+" "+dept);
	}

	public static void main(String[] args) {
		
		TestPc emp1= new TestPc();
		emp1.age= 23;
		emp1.name="Rajendra";
		emp1.dept="Automation";
		emp1.display();
	}

}
